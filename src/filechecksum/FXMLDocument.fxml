<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane id="borderPane" fx:id="borderPane" prefHeight="370.0" prefWidth="412.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="filechecksum.FXMLDocumentController">
    <top>
        <HBox spacing="10.0">
           <children>
               <TextField fx:id="TextFieldDirectory" promptText="Directory to scan" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </TextField>
               <Button fx:id="ButtonBrowse" onAction="#handleButtonBrowseAction" text="...">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
           </children>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
        </HBox>
    </top>
    <center>
        <VBox spacing="10.0">
           <children>
               <TableView VBox.vgrow="ALWAYS">
                   <columns>
                       <TableColumn text="Message" />
                       <TableColumn maxWidth="1.7976931348623157E308" minWidth="80.0" prefWidth="200.0" text="Path" />
                   </columns>
               </TableView>
               <ProgressBar maxWidth="1.7976931348623157E308" />
               <Button fx:id="button" layoutX="126" layoutY="90" text="Click Me!" />
               <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
           </children>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" />
         </BorderPane.margin>
        </VBox>
    </center>
